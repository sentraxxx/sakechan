# 辞書更新
{
    'event': 'updatedictionary' # 辞書更新
    'option': {
        'source': SAKETIMES     # 更新元
    }
}

# 辞書検索
{
    'event': 'query'
    'option': {
        'action': 'select',     # アクション
        'query': 'xxxx',        # 検索語(str)
        'limit': 'int',         # 検索上限(int)
        'orderby': 'yyyy',      # 並べ替え対象(str)
    }
}

# 辞書登録
{
    'event': 'updatedictionary'
    'option': {
        'action': 'update'      # 新規登録、更新
        'query': 'xxxx',        # 検索語(str)
        'from': 'yyyy',         # 検索対象(str)
        'limit': 'int',         # 検索上限(int)
        'orderby': 'yyyy',      # 並べ替え対象(str)
    }
}